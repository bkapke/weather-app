<md-content>
    <md-list flex>
        <md-subheader class="md-no-sticky"></md-subheader>
        <md-list-item class="md-3-line" ng-repeat="item in main.weather.list" ng-click="null">
        <img ng-src="http://openweathermap.org/img/w/{{item.weather[0].icon}}.png" class="md-avatar" alt="{{item.who}}" />
        <div class="md-list-item-text" layout="column">
            <h3><strong>High: </strong>{{ item.main.temp_max }} {{main.formatDate(item.dt_txt)}}</h3>
            <h3><strong>Low: </strong>{{ item.main.temp_min }}</h3>
            <p><strong>Humidity: </strong>{{ item.main.humidity }}</p>
        </div>
        </md-list-item>
    </md-list>
</md-content>
